generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WaitlistEntry {
  id        String   @id @default(cuid())
  email     String   @unique @db.VarChar(254)
  source    String?  @db.VarChar(50)
  createdAt DateTime @default(now())

  @@index([createdAt])
}